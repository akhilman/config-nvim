{
  "deno": {
    "enable": true,
    "lint": true
  },
  "pylsp": {
    "plugins": {
      "pylint.enabled": true,
      "pyflakes.enabled": false,
      "pyls_mypy.enabled": true,
      "pyls_mypy.live_mode": false,
      "isort.enabled": true
    }
  },
  "rust.clippy_preference": true,
  "rust-analyzer": {
    "cargo.features": "default",
    "inlineHints": {
      "enable": true,
      "chainingHints": true,
      "typeHints": true,
      "parameterHints": true
    }
  }
}
